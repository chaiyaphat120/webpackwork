{
    "presets":[
        "@babel/preset-react",
        [
            "@babel/preset-env",
            {
                "useBuiltIns": false,
                "targets": {
                    "node": "current"
                }
            }
        ]
    ], 
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "@babel/plugin-transform-runtime", {
                "corejs": false,
                "helpers": false,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ]
}